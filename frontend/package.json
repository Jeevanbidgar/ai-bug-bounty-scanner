{
  "name": "ai-bug-bounty-scanner-frontend",
  "version": "2.0.0",
  "description": "Vue.js frontend for AI Bug Bounty Scanner",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --open",
    "dev-debug": "vite --host 0.0.0.0 --debug",
    "dev-force": "vite --host 0.0.0.0 --force",
    "build": "vite build",
    "build-watch": "vite build --watch",
    "preview": "vite preview --host 0.0.0.0",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint-watch": "nodemon --watch src --ext vue,js,ts --exec \"npm run lint\"",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "restart": "npm run clean && npm install && npm run dev"
  },
  "dependencies": {
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.2.0",
    "date-fns": "^2.30.0",
    "js-file-download": "^0.4.12",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "nodemon": "^3.0.2",
    "vue-tsc": "^1.8.22",
    "@vue/tsconfig": "^0.4.0",
    "chokidar": "^3.5.3"
  }
}
